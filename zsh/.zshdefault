#!/usr/bin/env zsh

export ZSH_DISABLE_COMPFIX=true
export ZSH="$HOME/.oh-my-zsh"
export CLICOLOR=1
ZSH_THEME="refined"

export KEYTIMEOUT=20
bindkey -M viins 'kj' vi-cmd-mode

plugins=(
  git
  z
  zsh-syntax-highlighting
  zsh-autosuggestions
  vi-mode
  zsh-vimode-visual
)

source $ZSH/oh-my-zsh.sh
